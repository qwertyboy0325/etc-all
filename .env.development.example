# Development Environment Configuration Example
# Copy this file to .env for local development

# Database
POSTGRES_DB=etc_pointcloud_dev
POSTGRES_USER=root
POSTGRES_PASSWORD=root

# MinIO
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_BUCKET=pointcloud-files

# Application
SECRET_KEY=dev_secret_key_for_jwt_tokens_at_least_32_chars
BACKEND_CORS_ORIGINS=["*"]

# Admin User
FIRST_SUPERUSER_EMAIL=admin@etc.com
FIRST_SUPERUSER_PASSWORD=admin123
FIRST_SUPERUSER_NAME=Admin

# Paths
DATASET_EXPORT_PATH=/data/datasets

# Application Mode
DEBUG=True
ENVIRONMENT=development
